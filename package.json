  "scripts": {
    "start": "babel-watch server/app.js",
    "start:dev": "set NODE_ENV=development&& nodemon --watch server --exec babel-node server/app.js",
    "migrate": "set NODE_ENV=development&& babel-node server/models/index.js",
    "db-migrate": "set NODE_ENV=test&&babel-node server/models/index.js",
    "heroku-postbuild": "npm run migrate --env production && npm run build",
    "build": "babel server -d dist",
    "test": "set NODE_ENV=test&& babel-node server/models/index.js && nyc --reporter=html --reporter=text mocha --compilers js:babel-register ./server/tests/ --timeout 10000 --exit",
    "coveralls": "jscoverage lib && YOURPACKAGE_COVERAGE=1 nodeunit --reporter=lcov test | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build:client": "NODE_ENV=development  webpack --mode development",
    "start:client": "webpack-dev-server --mode development --open --hot"
  },
  "repository": {
    "type": "git",
